{
  "$schema": "http://json-schema.org/schema",
  "name": "func",
  "version": "0.0.1",
  "generators": {
    "init": {
      "factory": "./src/generators/init/generator",
      "schema": "./src/generators/init/schema.json",
      "description": "Initializes a new empty Azure functions app project"
    },
    "new": {
      "factory": "./src/generators/new/generator",
      "schema": "./src/generators/new/schema.json",
      "description": "Creates a new function inside an Azure function app project"
    }
  }
}
